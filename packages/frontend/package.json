{
  "name": "lewd.se-frontend-content",
  "version": "2.0.1",
  "description": "These are all the stylesheets and scripts for lewd.se",
  "main": "index.js",
  "scripts": {
    "build": "env dotenv_config_path=../../.env concurrently \"npm:build-js\" \"npm:build-css\"",
    "build-js": "parcel build ./src/js/*.js --out-dir ./dist/js",
    "build-css": "parcel build ./src/css/main.scss --out-dir ./dist/css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inabahare/lewd.se-frontend-content.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/inabahare/lewd.se-frontend-content/issues"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "babel-polyfill": "^6.26.0",
    "bulma": "^0.8.2",
    "bulmaswatch": "^0.7.5",
    "dotenv": "^8.2.0",
    "parcel": "^1.12.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^5.2.0",
    "parcel-bundler": "^1.12.4",
    "sass": "^1.26.5"
  }
}