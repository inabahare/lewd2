  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Antivirus Services",
      "preLaunchTask": "npm: build",
      "program": "${workspaceFolder}/dist/index.js",
      "protocol": "inspector",
      "sourceMaps": true,
      "outFiles": [
        "${workspaceFolder}/dist/**/*.js"
      ],
      "envFile": "${workspaceFolder}/../../.env",
      "runtimeArgs": [
        "--require=dotenv/config"
      ]
    }
  ]
}