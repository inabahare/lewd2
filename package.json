{
  "name": "lewd",
  "version": "2.0.0",
  "description": "This is the best uploader ever!!",
  "main": "app.js",
  "repository": "git@github.com:inabahare/lewd2.gitgit",
  "author": "inaba <software@panduro.guru>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "node dist/app.js",
    "debug": "node --nolazy --inspect-brk=9229 dist/app.js",
    "start-watch": "node_modules/.bin/concurrently \"npm:start-app\" \"npm:start-cron\"",
    "start-app": "nodemon --inspect dist/app.js",
    "start-cron": "nodemon dist/cronactions.js",
    "build": "node_modules/.bin/concurrently \"npm:build-server\" \"npm:build-scss\" \"npm:build-js\" && npm run move-sourcemaps",
    "build-server": "node_modules/.bin/parcel build src/app.js src/cronactions.js src/AntiVirusApi.js --target node --no-minify",
    "build-scss": "node_modules/.bin/parcel build Public/CSS/main.scss --out-dir Public/CSS/dist",
    "build-js": "node_modules/.bin/parcel build Public/JS/index.js Public/JS/main.js Public/JS/viewUploads.js --out-dir Public/JS/dist --target browser --no-minify",
    "build-watch": "node_modules/.bin/concurrently \"npm:watch-server\" \"npm:watch-less\"",
    "watch-server": "node_modules/.bin/parcel src/app.js src/cronactions.js --target node",
    "watch-less": "node_modules/.bin/parcel watch Public/CSS/main.less --out-dir Public/CSS",
    "watch-js": "node_modules/.bin/parcel watch Public/JS/index.js Public/JS/main.js Public/JS/viewUploads.js --out-dir Public/JS/dist --target browser",
    "move-sourcemaps": "mv dist/app.map dist/app.js.map"
  },
  "dependencies": {
    "append-field": "^1.0.0",
    "async": "^2.6.1",
    "axios": "^0.18.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^3.0.1",
    "body-parser": "^1.18.3",
    "bulma": "^0.7.4",
    "bulmaswatch": "^0.7.2",
    "busboy": "^0.2.11",
    "concat-stream": "^1.5.2",
    "cookie-parser": "^1.4.3",
    "cookie-session": "^2.0.0-beta.3",
    "debug": "^4.1.0",
    "del": "^3.0.0",
    "dnode": "^1.2.2",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-flash": "^0.0.2",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "express-validator": "^5.2.0",
    "fs-readfile-promise": "^3.0.1",
    "html-entities": "^1.2.1",
    "includes": "0.0.7",
    "js-string-escape": "^1.0.1",
    "mkdirp": "^0.5.1",
    "moment": "^2.22.2",
    "multer": "^1.3.0",
    "node-cron": "^1.2.1",
    "object-assign": "^4.1.1",
    "on-finished": "^2.3.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.4.3",
    "sha1": "^1.1.1",
    "type-is": "^1.6.4",
    "uuid": "^3.3.2",
    "xtend": "^4.0.0"
  },
  "devDependencies": {
    "@types/async": "^2.4.0",
    "@types/axios": "^0.14.0",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.16.1",
    "@types/express-flash": "0.0.0",
    "@types/multer": "^1.3.7",
    "@types/pg": "^7.4.11",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "concurrently": "^4.1.0",
    "parcel": "^1.12.3",
    "sass": "^1.17.0"
  }
}
